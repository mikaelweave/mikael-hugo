{
  "name": "mikael.dev",
  "version": "1.0.0",
  "description": "Statically generated website for Mikael Weaver's personal site.",
  "dependencies": {
    "autoprefixer": "^9.4.7",
    "postcss-cli": "^6.1.3"
  },
  "scripts": {
    "preview-local": "hugo serve -D",
    "preview-local-production": "hugo serve -D --environment production",
    "pull-srcsets": "python3 scripts/pull_srcsets_files.py",
    "pull-images": "python3 scripts/pull_images.py",
    "pull-code-files": "python3 scripts/pull_code_files.py",
    "push-changed-images": "python3 scripts/push_images.py",
    "push-all-images": "python3 scripts/push_images.py True",
    "build": "hugo --environment production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikaelweave/mikael-hugo.git"
  },
  "keywords": [
    "katyweaver",
    "hugo"
  ],
  "author": "Mikael Weaver",
  "license": "MIT",
  "homepage": "https://github.com/mikaelweave/mikael-hugo#readme"
}
